{
  "name": "ask-nugey",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.17.0"
  },
  "type": "module",
  "scripts": {
    "dev": "rm -rf .next && next dev",
    "build": "next build",
    "start": "next start",
    "dev:all": "run-p dev watch:posts",
    "----- format -----": "",
    "lint": "next lint --dir src",
    "format": "prettier --write --ignore-path .gitignore './src/**/*.{js,jsx,ts,tsx,json,css}'",
    "fix": "prettier --write --ignore-path .gitignore './src/**/*.{js,jsx,ts,tsx,json,css}' && next lint --fix --dir src",
    "----- worker -----": "",
    "build:worker": "opennextjs-cloudflare build",
    "preview:worker": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy:worker": "npm run generate:posts && opennextjs-cloudflare build && opennextjs-cloudflare deploy -- --minify",
    "deploy-dry:worker": "npm run generate:posts && opennextjs-cloudflare build && opennextjs-cloudflare deploy -- --minify --dry-run",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "----- other -----": "",
    "generate:posts": "node scripts/generatePostSlugs.js",
    "watch:posts": "node scripts/watchPostSlugs.js",
    "prepare↓  (MEMO: install後に以下scriptを実行)": "",
    "prepare↓  (MEMO: `panda codegen` で panda.config.ts を元に styled-system/ を生成)": "",
    "prepare": "panda codegen"
  },
  "dependencies": {
    "@giscus/react": "^3.0.0",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@mistralai/mistralai": "^1.5.1",
    "@next/mdx": "15.3.1",
    "@types/file-saver": "^2.0.7",
    "antd": "^5.22.3",
    "date-fns": "4.1.0",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "next": "14.2.28",
    "node-html-parser": "7.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "^10.1.0",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.5.2",
    "@cloudflare/workers-types": "^4.20241205.0",
    "@csstools/postcss-cascade-layers": "^4.0.6",
    "@eslint/compat": "^1.2.3",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@opennextjs/cloudflare": "0.6.6",
    "@pandacss/dev": "^0.41.0",
    "@types/node": "22.15.3",
    "@types/react": "18.3.20",
    "@types/react-dom": "18.3.7",
    "eslint": "9.14.0",
    "eslint-config-next": "^14.2.18",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jiti": "^2.4.1",
    "mdx-mermaid": "2.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.1",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-code-titles": "^1.2.0",
    "rehype-external-links": "^3.0.0",
    "rehype-katex": "^7.0.1",
    "rehype-prism-plus": "^2.0.1",
    "rehype-shift-heading": "^2.0.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-table-of-contents": "^2.1.0",
    "remark-toc": "^9.0.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "wrangler": "4.9.1"
  }
}
